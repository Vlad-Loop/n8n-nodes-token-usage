{
  "name": "n8n-nodes-openrouter-token-tracker",
  "version": "1.0.0",
  "description": "n8n custom node for tracking OpenRouter AI API token costs",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "openrouter",
    "token",
    "cost",
    "tracker",
    "ai",
    "llm",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Vlad-Loop/n8n-nodes-openrouter-token-tracker",
  "author": {
    "name": "Vlad Loop",
    "email": "v.tarasenko1997@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vlad-Loop/n8n-nodes-openrouter-token-tracker.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenRouterApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TokenCostTracker/TokenCostTracker.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.19.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.19.0"
  }
}
