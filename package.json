{
  "name": "n8n-nodes-token-usage",
  "version": "1.0.0",
  "description": "n8n community node for tracking LLM token usage across multiple providers (OpenAI, Anthropic, Google, and more)",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "token",
    "ai",
    "llm",
    "usage",
    "pricing",
    "cost",
    "openrouter"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Vlad-Loop/n8n-nodes-token-usage",
  "author": {
    "name": "Vlad Loop",
    "email": "v.tarasenko1997@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vlad-Loop/n8n-nodes-token-usage.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint 'nodes/**/*.ts' package.json",
    "lintfix": "eslint 'nodes/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/TokenUsage/TokenUsage.node.js",
      "dist/nodes/AutoPriceCalculator/AutoPriceCalculator.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.19.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.19.0"
  }
}
